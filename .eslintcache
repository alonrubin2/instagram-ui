  }\n        setUser(user);\n      }\n      catch (err) {\n        console.log(err);\n      }\n    }\n    getMe();\n  }, [history]);\n\n  function isLogged() {\n    return Boolean(Object.keys(user).length);\n  }\n\n\n\n\n\n  return (\n    <UserContext.Provider value={{ user: user, setUser }}>\n      <div className=\"App\">\n        { isLogged() && <Header /> }\n        <MapContext.Provider value={{ north: '', setNorth}, {east: '', setEast }}>\n\n        <div className=\"container\">\n          <Switch>\n            <Route path=\"/register\">\n              <Register />\n            </Route>\n            <Route path=\"/login\">\n              <Login />\n            </Route>\n            <Route path=\"/search\">\n              <UserSearch />\n            </Route>\n            <Route path=\"/user/edit\">\n              <EditProfile />\n            </Route>\n            <Route path=\"/user/:username\">\n              <Profile />\n            </Route>\n            <Route path=\"/post/create\">\n              <PostCreate />\n            </Route>\n            <Route path=\"/post/:id\">\n              <PostPage />\n            </Route>\n            <Route path=\"/\" exact>\n              <Feed />\n            </Route>\n          </Switch>\n        </div>\n        </MapContext.Provider>\n\n      </div>\n    </UserContext.Provider>\n\n  );\n}\n\nexport default App;\n","C:\\Users\\Alon\\Desktop\\NetCraft\\Projects\\final-project\\instagram-ui\\src\\Header\\Header.js",[],"C:\\Users\\Alon\\Desktop\\NetCraft\\Projects\\final-project\\instagram-ui\\src\\Register\\Register.js",[],"C:\\Users\\Alon\\Desktop\\NetCraft\\Projects\\final-project\\instagram-ui\\src\\Register\\register.schema.js",[],"C:\\Users\\Alon\\Desktop\\NetCraft\\Projects\\final-project\\instagram-ui\\src\\Login\\Login.js",[],"C:\\Users\\Alon\\Desktop\\NetCraft\\Projects\\final-project\\instagram-ui\\src\\Login\\login.schema.js",[],"C:\\Users\\Alon\\Desktop\\NetCraft\\Projects\\final-project\\instagram-ui\\src\\Feed\